{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/text-alternatives": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 127",
    "not firefox <= 130",
    "not ios_saf <= 18",
    "not safari <= 18"
  ]
}